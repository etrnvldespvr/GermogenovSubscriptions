<Window x:Class="GermogenovSubscriptions.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GermogenovSubscriptions"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="600">
    <Grid>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" />
        <Button Content="Отправиться на дату продления" Style="{StaticResource FlatButtonStyle}" Width="200" Height="30" Margin="0,10,10,10" HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="SimulateRenewalDate_Click"/>
        <TextBlock Text="Dev build 0.15" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10,0,0,10" Foreground="{StaticResource TextColor}" FontSize="10"/>
    </Grid>
</Window>
